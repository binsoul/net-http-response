{
    "name": "binsoul/net-http-response",
    "description": "Specialized response and body classes plus response emitter",
    "keywords": [
        "http",
        "response"
    ],
    "homepage": "https://github.com/binsoul/net-http-response",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Mößler",
            "email": "code@binsoul.de",
            "homepage": "https://github.com/binsoul",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "binsoul/common": "~1.0@dev",
        "binsoul/io-stream": "~1.0@dev",
        "binsoul/net": "~1.0@dev",
        "binsoul/net-http-message-bridge": "~1.0@dev",
        "binsoul/net-http-message-message": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "fabpot/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BinSoul\\Net\\Http\\Response\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinSoul\\Test\\Net\\Http\\Response\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-src": "php-cs-fixer fix -v src",
        "fix-tests": "php-cs-fixer fix -v tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
